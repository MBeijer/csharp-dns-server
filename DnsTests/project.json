{
  "dependencies": {
    "Dns": "",
    "xunit": "2.1.0-*",
    "xUnit.Assert": "2.1.0-*",
    "xunit.core":"2.1.0-*",
    "xunit.extensibility.core": "2.1.0-*",
    "xunit.runner.aspnet": "2.1.0-*"
  },
  
  "commands": {
    "test": "xunit.runner.aspnet"
  },
  
  "runtimes": {
    "win": { },
    "win-anycpu": {
      "#import": [ "win" ]},
    "win7": { },
    "win7-anycpu": { },
    "ubuntu.14.04": { },
    "ubuntu.14.04-anycpu": { },
    "osx.10.10": { },
    "osx.10.10-anycpu": { }
  },
  
  "frameworks": {
    ".NETFramework,Version=v4.5":{
      "frameworkAssemblies": {
          "System.Runtime":"",
          "System.Runtime.Caching":""
          }
    },
    "dnx451": {
      "dependencies": {
            "xunit.runner.dnx": "2.1.0-*"
      },
      
      "frameworkAssemblies": {
        "System.Runtime":"",
        "System.Runtime.Caching": ""
      }
    }
  },
  
  "compile": [
    "*.cs",
    "**/*.cs"
  ],
  
  "compilationOptions": {
    "warningsAsErrors": true
  }
}
