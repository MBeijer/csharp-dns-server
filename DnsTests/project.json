{
    "dependencies": {
        "Dns": "1.0.0-*",
        "xunit": "2.1.0",
        "xUnit.Assert": "2.1.0-*",
        "xunit.core": "2.1.0-*",
        "xunit.extensibility.core": "2.1.0-*",
        "xunit.runner.dnx": "2.1.0-*"
    },
    "runtimes": {
        "win": {},
        "win-anycpu": {
            "#import": [
                "win"
            ]
        },
        "win7": {},
        "win7-anycpu": {},
        "ubuntu.14.04": {},
        "ubuntu.14.04-anycpu": {},
        "osx.10.10": {},
        "osx.10.10-anycpu": {}
    },
  
    "commands": {
        "test": "xunit.runner.dnx"
    },
    "frameworks": {
        "dnx451": {
            "frameworkAssemblies": {
                "System.Runtime": "",
                "System.Runtime.Caching": ""
            }
        }
    },
    "compile": [
        "*.cs",
        "**/*.cs"
    ],
    "compilationOptions": {
        "warningsAsErrors": true
    }
}