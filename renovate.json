{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:all"
  ],
  "baseBranches": [
    "master"
  ],
  "forkProcessing": "enabled",
  "packageRules": [
    {
      "matchManagers": ["nuget"],
      "matchPackageNames": ["AutoMapper"],
      "allowedVersions": "<15.0.0"
    }
  ]
}
