{
  "server": {
    "zones": [
      {
        "name": ".stephbu.org",
        "provider": "Dns.ZoneProvider.IPProbe.IPProbeZoneProvider",
        "providerSettings": {
          "$type": "ipprobe",
          "pollingIntervalSeconds": 15,
          "hosts": [
            {
              "name": "foo",
              "probe": "ping",
              "timeout": 30,
              "availabilityMode": "first",
              "ip": [
                "192.168.1.1",
                "192.168.1.252",
                "192.168.1.253",
                "192.168.86.100"
              ]
            },
            {
              "name": "test",
              "probe": "http",
              "timeout": 100,
              "availabilityMode": "all",
              "ip": [
                "192.168.86.1",
                "192.168.1.1"
              ]
            }
          ]
        }
      },
      {
        "name": ".stephbu.org",
        "provider": "Dns.ZoneProvider.Traefik.TraefikZoneProvider",
        "providerSettings": {
          "$type": "traefik",
          "traefikUrl": "https://traefik-address",
          "username": "",
          "password": "",
          "dockerHostInternalIp": "192.168.0.10"
        }
      },
      {
        "provider": "Dns.ZoneProvider.DatabaseZoneProvider"
      }
    ],
    "dnsListener": {
      "port": 5335
    },
    "webServer": {
      "JwtSecretKey": ""
    }
  },
  "databaseSettings": {
    "sqliteDefault": "Data Source=dns.db"
  }
}